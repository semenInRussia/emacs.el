;;; Commentary:

;; My configuration of `god-mode'.

;;; Code:

(require 'my-leaf)

(leaf god-mode
  :ensure t
  :defun god-mode
  :bind ("M-SPC" . god-mode)
  :config (god-mode))

(provide 'my-god-mode)
